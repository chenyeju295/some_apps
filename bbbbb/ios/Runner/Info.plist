<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Bbbbb</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>bbbbb</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	
	<!-- Privacy - Camera Usage Description -->
	<key>NSCameraUsageDescription</key>
	<string>DiveExplorer needs camera access to capture your diving adventures and generate stunning underwater-themed images with AI technology.</string>
	
	<!-- Privacy - Microphone Usage Description -->
	<key>NSMicrophoneUsageDescription</key>
	<string>DiveExplorer uses microphone access to record audio for your diving logs and underwater adventure videos.</string>
	
	<!-- Privacy - Photo Library Usage Description -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>DiveExplorer needs access to your photo library to save generated ocean images, share diving memories, and import photos for AI enhancement.</string>
	
	<!-- Privacy - Photo Library Add Usage Description -->
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>DiveExplorer saves your AI-generated underwater images and diving photos to your photo library for easy sharing and memories.</string>
	
	<!-- Privacy - Local Network Usage Description -->
	<key>NSLocalNetworkUsageDescription</key>
	<string>DiveExplorer uses local network access to connect with diving equipment, underwater cameras, and share content with nearby dive buddies.</string>
	
	<!-- Privacy - Location When In Use Usage Description -->
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>DiveExplorer uses your location to find nearby dive sites, log dive locations, and provide location-based marine life information.</string>
	
	<!-- Privacy - Location Always and When In Use Usage Description -->
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>DiveExplorer uses location services to track dive sites, provide marine conservation updates, and enhance your underwater exploration experience.</string>
	
	<!-- App Transport Security Settings -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
	
	<!-- Privacy - App Tracking Transparency Usage Description -->
	<key>NSUserTrackingUsageDescription</key>
	<string>DiveExplorer uses tracking to deliver personalized ocean content, suggest relevant dive sites, and provide tailored marine life experiences. Your privacy is important to us.</string>
</dict>
</plist>
